
package com.ss.criteria;

/**
 * [活动评论搜索类]<p>
 * [功能详细描述]<p>
 * @author zzb
 * @version 1.0, 2016年1月21日
 * @see
 * @since zhubu-V1001
 */
public class CommentActivityCriteria extends AbstractQueryCriteria
{
    /**
     * 评论人姓名
     */
    private String name;

    /**
     * 评论人头像
     */
    private String icon;

    public String getName()
    {
        return name;
    }

    public void setName(String name)
    {
        this.name = name;
    }

    public String getIcon()
    {
        return icon;
    }

    public void setIcon(String icon)
    {
        this.icon = icon;
    }

}
